    "@nuxt/types": "^0.7.4",
    "@nuxt/typescript": "^2.8.1",
    "@nuxt/typescript-build": "^0.3.3",
    "@nuxt/vue-app": "^2.15.8",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/google-analytics": "^2.2.1",
    "@nuxtjs/toast": "^3.3.0",
    "@nuxtjs/vuetify": "^1.11.0",
    "@types/node": "^16.11.6",
    "@yarnpkg/pnpify": "^3.1.1-rc.1",
    "core-js": "^2.6.11",
    "nodemon": "^1.18.9",
    "object-fit-images": "^3.2.4",
    "vue-analytics": "^5.17.2",
