{
  "name": "nuxt-ts-hello",
  "version": "1.0.0",
  "description": "My great Nuxt.js project",
  "author": "EinYs",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOST=127.0.0.1 PORT=8080 nodemon server/index.js --watch server",
    "build": "npm run nuxt-build-prod",
    "nuxt-build-dev": "cross-env NODE_ENV=development HOST=127.0.0.1 PORT=8080 nuxt build",
    "nuxt-build-prod": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=8080 nuxt build",
    "start": "cross-env NODE_ENV=production HOST=0.0.0.0 PORT=8080 node server/index.js",
    "pm2": "sudo pm2 start ecosystem.config.js --env production",
    "pm2-dev": "pm2 start ecosystem.config.js",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "browserslist": "browserslist",
    "docker-up": "docker-compose up -d",
    "docker-up2": "docker-compose up -d --scale node-app=2",
    "docker-build": "docker buildx build --platform linux/amd64,linux/arm64 -t einys/cmsn --push ."
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE >= 10"
  ],
  "dependencies": {
    "@einys/snbot": "^3.2.0",
    "axios": "^0.21.1",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.0.0",
    "consola": "^2.15.3",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "debug": "~2.6.9",
    "defu": "^5.0.0",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "nuxt": "^2.12.2",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "request": "^2.88.0",
    "snbot": "^1.1.1",
    "twit": "^2.2.11",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/runtime-corejs2": "^7.9.2",
